&gem0 {
	status = "okay";
	phy-mode = "rgmii-id";
	phy-handle = <&phy0>;

	mdio {
		phy0: phy@3 {
			reg = <3>;
			txc-skew-ps = <1800>;
			txen-skew-ps = <420>;

			txd0-skew-ps = <420>;
			txd1-skew-ps = <420>;
			txd2-skew-ps = <420>;
			txd3-skew-ps = <420>;

			rxc-skew-ps = <900>;
			rxdv-skew-ps = <420>;

			rxd0-skew-ps = <420>;
			rxd1-skew-ps = <420>;
			rxd2-skew-ps = <420>;
			rxd3-skew-ps = <420>;		
		};

      // GEM3 disabled due to issue with SFP0 link negotiation
      /*
		phy1: phy@7 {
			reg = <7>;

			txc-skew-ps = <1800>;
			txen-skew-ps = <420>;

			txd0-skew-ps = <420>;
			txd1-skew-ps = <420>;
			txd2-skew-ps = <420>;
			txd3-skew-ps = <420>;

			rxc-skew-ps = <900>;
			rxdv-skew-ps = <420>;

			rxd0-skew-ps = <420>;
			rxd1-skew-ps = <420>;
			rxd2-skew-ps = <420>;
			rxd3-skew-ps = <420>;		
		};
      */
	};
};

// GEM3 disabled due to issue with SFP0 link negotiation
&gem3 {
	status = "disabled";
	//phy-handle = <&phy1>;
	//phy-mode = "rgmii-id";
};

&qspi {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	is-dual = <0>;
	num-cs = <1>;
	flash0: flash@0 {
		reg = <0x0>;
		compatible = "spansion,s25fl512s", "jedec,spi-nor";
		spi-rx-bus-width = <0x4>;
		spi-tx-bus-width = <0x4>;
		spi-max-frequency = <50000000>;
		#address-cells = <1>;
		#size-cells = <1>;
	};
};

&psgtr {
	status = "okay";
	clocks = <&ref100>, <&ref27>;
	clock-names = "ref2", "ref3";
};

&amba {
	ref100: ref100M {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <100000000>;
	};
	ref27: ref27M {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <27000000>;
	};
	i2c_mgmt: i2c@ff020000 {
		status = "okay";
		clock-frequency = <100000>;
		atsha204a@64 {
			status = "okay";
			compatible = "atmel,atsha204a", "atmel,atsha204-i2c";
			reg = <0x64>;
		};
	};
	i2c_user: i2c@ff030000 {};
};

&A4LITE_00 {
   compatible = "generic-uio";
};
